{
  "buildCommand": "npm run build:frontend",
  "functions": {
    "server.js": {
      "includeFiles": [
        "index.html",
        "style.css",
        "script.js",
        "env.js"
      ]
    }
  },
  "headers": [
    {
      "source": "/env.js",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store, max-age=0"
        }
      ]
    }
  ]
}
